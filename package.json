{
  "name": "eslintcc",
  "version": "0.4.2",
  "homepage": "https://eslintcc.github.io",
  "description": "ESLintCC is a ECMAScript/JavaScript tool that computes complexity of code by using ESLint",
  "keywords": [
    "eslint",
    "javascript",
    "complexity",
    "linter",
    "static-code-analysis"
  ],
  "repository": "github:eslintcc/eslintcc",
  "license": "MIT",
  "main": "./source/complexity.js",
  "bin": {
    "eslintcc": "eslintcc"
  },
  "files": [
    "source",
    "eslintcc"
  ],
  "dependencies": {
    "eslint": "7.6.0",
    "@ndk/env": "0.0.3"
  },
  "devDependencies": {
    "semver": "7.3.2",
    "nyc": "15.1.0",
    "coveralls": "3.1.0",
    "@ndk/test": "0.0.4"
  },
  "scripts": {
    "test": "node bin/test",
    "coverage": "node bin/coverage",
    "coverage-all": "node bin/coverage-all"
  },
  "nyc": {
    "all": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "exclude": [
      "bin",
      "coverage",
      "test"
    ]
  }
}