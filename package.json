{
  "name": "eslintcc",
  "version": "0.5.4",
  "homepage": "https://eslintcc.github.io",
  "description": "ESLintCC is a ECMAScript/JavaScript tool that computes complexity of code by using ESLint",
  "keywords": [
    "eslint",
    "javascript",
    "complexity",
    "linter",
    "static-code-analysis"
  ],
  "repository": "github:eslintcc/eslintcc",
  "license": "MIT",
  "main": "./source/complexity.js",
  "bin": {
    "eslintcc": "eslintcc"
  },
  "files": [
    "source",
    "eslintcc"
  ],
  "dependencies": {
    "eslint": "7.17.0"
  },
  "devDependencies": {
    "semver": "7.3.4",
    "nyc": "15.1.0",
    "coveralls": "3.1.0",
    "rollup": "^2.36.1",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@nodutilus/test": "^0.2.1"
  },
  "scripts": {
    "build": "npx rollup --config",
    "test": "node bin/test",
    "coverage": "node bin/coverage",
    "coverage-all": "node bin/coverage-all"
  },
  "nyc": {
    "all": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "exclude": [
      "bin",
      "build",
      "coverage",
      "test",
      "rollup.config.js",
      "source/lib/args.js"
    ]
  }
}